	.section3>.box a{display:block; width:132px; height:31px; margin:63px 0 0 141px; border:1px solid #a8b1ab; color:#fff; font-size:12px; line-height:31px;}
	div.btn a{display:block; width:60px; height:60px; background:url(../images/main/btn_top.png)no-repeat; margin-left:1375px; text-indent:-9999px;}
	
	
	/* footer 영역 */
	footer{height:212px; background:#0e1513;}
	.util{position:relative; width:1200px; margin:0 auto;}
	.util>.foot_menu{width:969px;  margin-left:28px; padding-bottom:12px; border-bottom:1px solid #2a2f2d; *zoom:1;}
