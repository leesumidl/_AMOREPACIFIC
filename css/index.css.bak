	/* article 슬라이드 이미지 */
	
	#slides {position:relative; width:100%; min-width:1000px; height:600px; margin:0 auto; overflow:hidden; background:#8c908c;}
	#slides ul{position: relative; width:100%; height:600px;}
	#slides>.visual>li{position:absolute; left:0; width:100%; height:600px; background-repeat:no-repeat; background-position: 50% 10%;}
	#slides>.visual>li a{display:block; width:132px; height:31px; font-size:12px; text-align:center; line-height:31px;}
    
	/* 슬라이드 공통 영역 */
	#slides>.visual>li>.tit{font-size:35px; letter-spacing:-0.09em;}
	#slides>.visual>li>.tx1{font-size:18px; letter-spacing:-0.06em;}
	#slides>.visual>li>.tx2{font-size:15px; letter-spacing:-0.035em;}

	
	/* 슬라이드 1 */
	#slides>.visual>.visual_1{background:url(../images/main/visual_1.jpg)no-repeat; color:#fff;}
	#slides>.visual>.visual_1>.img{position:relative; top:140px; left:57%;}
	#slides>.visual>.visual_1>.tit{position:relative; top:143px; left:57%;}
	#slides>.visual>.visual_1>.tx1{position:relative; top:153px; left:57%;}
	#slides>.visual>.visual_1>.tx2{position:relative; top:180px; left:57%;}
	#slides>.visual>.visual_1>a{border:1px solid #fff; color:#fff; position:relative; top:219px; left:57%;}
	/* 슬라이드 2 */
	#slides>.visual>.visual_2{background:url(../images/main/visual_2.jpg);}
	#slides>.visual>.visual_2>.img{position:relative; top:115px; left:58%;}
	#slides>.visual>.visual_2>.tit{position:relative; top:118px; left:58%; color:#0c0d22;}
	#slides>.visual>.visual_2>.tx1{position:relative; top:126px; left:58%; color:#38454e;}
	#slides>.visual>.visual_2>.tx2{position:relative; top:160px; left:58%; color:#685337;}
	#slides>.visual>.visual_2>a{border:1px solid #788882;color:#0c221c; position:relative; top:220px; left:58%;}
	/* 슬라이드 3 */
	#slides>.visual>.visual_3{background:url(../images/main/visual_3.jpg);}
	#slides>.visual>.visual_3>.tit{position:relative; top:147px; left:57%; color:#35291a; }
	#slides>.visual>.visual_3>.tx1{position:relative; top:153px; left:57%; color:#624d32;}
	#slides>.visual>.visual_3>.tx2{position:relative; top:184px; left:57%; color:#644d30;}
	#slides>.visual>.visual_3>a{border:1px solid #74725d; color:#1b2d25; position:relative; top:245px; left:57%;}
	/* 슬라이드 4 */
	#slides>.visual>.visual_4{background:url(../images/main/visual_4.jpg);}
	#slides>.visual>.visual_4>.tit{position:relative; top:141px; left:57%; color:#fff;}
	#slides>.visual>.visual_4>.tx1{position:relative; top:148px; left:57%; color:#fff;}
	#slides>.visual>.visual_4>.tx2{position:relative; top:180px; left:57%; color:#fff;}
	#slides>.visual>.visual_4>a{border:1px solid #7f7f7f; color:#fff; position:relative; top:245px; left:57%;}
	/* 슬라이드 5 */
	#slides>.visual>.visual_5{background:url(../images/main/visual_5.jpg);}
	#slides>.visual>.visual_5>.tit{position:relative; top:140px; left:57%; color:#0c221c;}
	#slides>.visual>.visual_5>.tx1{position:relative; top:148px; left:57%; color:#3b5149;}
	#slides>.visual>.visual_5>.tx2{position:relative; top:180px; left:57%; color:#384e46;}
	#slides>.visual>.visual_5>a{border:1px solid #678078; color:#4a574f; position:relative; top:240px; left:57%;}
	

	/* article 버튼 */
	#slides>.button{position:absolute; left:51.5%; bottom:30px; width:130px; height:14px; margin-left:-70px; z-index:1000; *zoom:1;}
	#slides>.button:after{display:block; clear:both; content:'';}
	#slides>.button>li{float:left; margin-right:10px;}
	#slides>.button>li:last-child{margin-right: 0;}
	#slides>.button>li.on{width:25px; height:12px; background-color:#ced2d1; border-radius:8px;}
	#slides>.button>li a{display:block; width:12px; height:12px; text-indent:-9999px; background-color:#ced2d1; border-radius:8px; transition:all .05s;}
	
	/* 이전/다음버튼 */
    #slides>.slides-navigation{display:inline-block; position:absolute; top:50%; width:30px; height:58px; margin-top:-29px; text-indent:-9999px; background-image:url(../images/main/btn_arrow.png); background-repeat:no-repeat; z-index:1000;}
	#slides>.slides-prev{left:30px; background-position:-5px -141px;}
	#slides>.slides-prev:hover{background-position:-5px -209px;}
	#slides>.slides-next{right:30px; background-position:-5px -5px;}
	#slides>.slides-next:hover{background-position:-5px -73px;}
	
	/*자동재생버튼*/
	#slides>.slides-btn{position:absolute; left:50%; bottom:28px; margin-left:80px; display:block; width:20px; height:20px; text-indent:-9999px; background-image:url(../images/main/icons.png); background-repeat:no-repeat; background-position:-62px -24px; z-index:1000;}
	#slides>.slides-btn.pause{background-position:0px -57px;}

	/* section1 영역 */
	.section1{width:1200px; height:560px; margin:50px auto 0; *zoom:1;}
	.section1:after{display:block; clear:both; content:'';} 
	.section1>.con1{color:#fff;}
	.section1>.con1_1 a{display:block;}
	.section1>.con1>li{float:left; width:600px; height:560px;}

	.section1>.con1>.con1_1{background:url(../images/main/film.jpg)no-repeat; text-align:center;}
	.section1>.con1>.con1_1:hover{opacity:0.8;}
	.section1>.con1>.con1_1 h2,.section1>.con1>.con1_2 h2{padding-top:203px; font-size:30px; font-weight:100; color:#fff;}
	.section1>.con1>.con1_1 p,.section1>.con1>.con1_2 p{margin-top:10px;font-size:14px; letter-spacing:1.5px; font-family:'Myriad Pro Light'; color:#fff;}
	.section1>.con1>.con1_1>.txt,.section1>.con1>.con1_2>.txt{display:block; width:132px; height:31px; margin:60px 0 0 233px; border:1px solid #a8b1ab; font-size:12px; text-align:center; line-height:31px; color:#fff;}
	.section1>.con1>.con1_2{background:url(../images/main/artbook.jpg)no-repeat; text-align:center;}
	.section1>.con1>.con1_2:hover{opacity:0.8;}


	
	/* section2 영역 */
	.section2{width:1200px; height:490px; margin:50px auto 0;}
	.section2>.con2{*zoom:1;}
	.section2>.con2:after{display:block; clear:both; content:'';}
	.section2>.con2>li{float:left; width:370px; height:490px; margin-right:45px; text-align:center; color:#fff;}
	.section2>.con2>.con2_1{background:#0c221c;}
	.section2>.con2>.con2_1>.txt strong,.section2>.con2>.con2_2>.txt strong,.section2>.con2>.con2_3>.txt strong
	{display:block; margin-top:46px; line-height:29px;font-size:25px; font-weight:100; letter-spacing:-0.12em; font-style:normal; font-family:'Myriad Pro Light';}
	.section2>.con2>.con2_1>.txt p,.section2>.con2>.con2_2>.txt p,.section2>.con2>.con2_3>.txt p{margin-top:5px; font-size:15px; letter-spacing:-0.07em; font-family:'Myriad Pro Light';}
	.section2>.con2>.con2_1>.txt a,.section2>.con2>.con2_2>.txt a,.section2>.con2>.con2_3>.txt a{display:block; width:132px; height:31px; margin:47px 0 0 118px; color:#fff; border:1px solid #a8b1ab; font-size:12px; line-height:31px;}
	.section2>.con2>.con2_1>.img1{height:245px; background:url(../images/main/highlights_1.jpg)no-repeat;}

	.section2>.con2>.con2_2{background:#7b704d;}
	.section2>.con2>.con2_2>.img2{height:245px; background:url(../images/main/highlights_2.jpg)no-repeat;}
	.section2>.con2>.con2_2>.txt p{line-height:17px; margin-top:8px; letter-spacing:-0.05em;}
	.section2>.con2>.con2_2>.txt a{margin-top:45px;}
	.section2>.con2>.con2_3{background:#262626; margin-right:0;}
	.section2>.con2>.con2_3>.img3{height:245px; background:url(../images/main/highlights_3.jpg)no-repeat;}
	
	
	/* section3 영역 */
	.section3{width:1200px; height:510px; margin:50px auto 70px; background:url(../images/main/advisor.jpg)no-repeat; position:relative;}
	.section3>.box{width:416px; height:296px; position:absolute; top:107px; left:392px; font-style:normal; background:#1f322d; opacity:0.9; text-align:center; color:#fff;}
